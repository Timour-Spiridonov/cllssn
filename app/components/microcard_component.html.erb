   <div class="card-product">
        <% if @product.image.attached?%>
            <%= image_tag @product.image %>
        <% else%>
            <img src="https://images.unsplash.com/photo-1475178626620-a4d074967452?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1372&q=80" alt="">
        <%end%>
        <p class="card-title">
        <%= @product.name%>
        </p>
        <p class="card-price">
            <%= @product.price %> DT
        </p>
    </div>
